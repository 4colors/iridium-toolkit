<?xml version="1.0"?>
<block>
  <name>pdu_round_robin</name>
  <key>iridium_toolkit_pdu_round_robin</key>
  <category>iridium_toolkit</category>
  <import>import iridium_toolkit</import>
  <make>iridium_toolkit.pdu_round_robin($output_count)</make>

  <param>
    <name>Output Count</name>
    <key>output_count</key>
	<value>2</value>
    <type>int</type>
  </param>

  <sink>
    <name>cpdus</name>
    <type>message</type>
  </sink>

  <source>
    <name>out_0</name>
    <type>message</type>
  </source>

  <source>
    <name>out_1</name>
    <type>message</type>
  </source>

</block>
